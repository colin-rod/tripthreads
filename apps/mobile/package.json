    "@types/react": "~19.2.8",
    "detox": "^20.46.3",
    "detox-expo-helpers": "^0.6.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "nativewind": "^4.2.1",
    "react-test-renderer": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.2"
